        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<system.serviceModel>
		<client>
			<endpoint name="connect_server"
                      address="net.tcp://localhost:8001/Server"
                      binding="netTcpBinding"
                      contract="Common.IElectricityConsumption" />
		</client>
